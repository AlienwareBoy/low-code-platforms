{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/apis/*": ["src/apis/*"],
      "@/assets/*": ["src/assets/*"],
      "@/components/*": ["src/components/*"],
      "@/layout/*": ["src/layout/*"],
      "@/pages/*": ["src/pages/*"],
      "@/plugins/*": ["src/plugins/*"],
      "@/router/*": ["src/router/*"],
      "@/store/*": ["src/store/*"],
      "@/utils/*": ["src/utils/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"]
}
